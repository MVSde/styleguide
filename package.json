{
  "name": "front-end-styleguide",
  "version": "3.7.3",
  "description": "Living Styleguide for componentized front end development with Sass, JavaScript and Nunjucks.",
  "keywords": [
    "babel",
    "css",
    "es6",
    "front-end",
    "html",
    "javascript",
    "nunjucks",
    "sass",
    "styleguide"
  ],
  "author": {
    "name": "Fynn Becker",
    "email": "info@fynn-becker.de"
  },
  "homepage": "https://front-end-style.guide",
  "repository": {
    "type": "git",
    "url": "https://github.com/front-end-styleguide/core"
  },
  "bugs": {
    "url": "https://github.com/front-end-styleguide/core/issues",
    "email": "help@front-end-style.guide"
  },
  "scripts": {
    "lint": "stylelint **/*.scss && eslint **/*.js",
    "pretest": "npm run lint",
    "test": "cd test/basic && node ../../bin/front-end-styleguide.js build:dev build:prev build",
    "watchtest": "cd test/basic && node ../../bin/front-end-styleguide.js dev",
    "linttest": "cd test/basic && node ../../bin/front-end-styleguide.js test"
  },
  "main": "index.js",
  "bin": "bin/front-end-styleguide.js",
  "dependencies": {
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.13",
    "browserify": "^14.4.0",
    "chalk": "^2.1.0",
    "chokidar": "^1.7.0",
    "envify": "^4.1.0",
    "fs-extra": "^4.0.1",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sass-vars": "^1.3.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-stylelint": "^5.0.0",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "nunjucks": "^3.0.1",
    "run-sequence": "^2.1.0",
    "through2": "^2.0.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "normalize.css": "^7.0.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "svgxuse": "^1.2.4"
  },
  "engines": {
    "node": ">= 7.0.0"
  },
  "license": "GPL-3.0"
}
