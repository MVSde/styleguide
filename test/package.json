{
  "name": "front-end-styleguide-test",
  "version": "0.0.0",
  "scripts": {
    "dev": "node ../bin/front-end-styleguide.js dev",
    "build": "node ../bin/front-end-styleguide.js build",
    "build:dev": "node ../bin/front-end-styleguide.js build:dev"
  },
  "ava": {
    "verbose": true,
    "files": [
      "test/unit"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "exclude": [
      "src/main.js"
    ],
    "reporter": [
      "text",
      "text-summary",
      "html"
    ]
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "normalize.css": "^7.0.0"
  }
}
