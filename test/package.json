{
  "name": "pangolin-test",
  "version": "0.0.0",
  "scripts": {
    "dev": "node ../bin/pangolin.js dev",
    "build": "node ../bin/pangolin.js build",
    "build:dev": "node ../bin/pangolin.js build:dev"
  },
  "ava": {
    "verbose": true,
    "files": [
      "test/unit"
    ]
  },
  "nyc": {
    "all": true,
    "require": [
      "babel-register"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "src/main.js"
    ],
    "reporter": [
      "text",
      "text-summary",
      "html"
    ]
  },
  "dependencies": {
    "es6-promise": "^4.2.4",
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1"
  }
}
